@page
@model Exwhyzee.AANI.Web.Areas.Main.Pages.SecFamilies.ParticipantMember.EditModel

@{
    ViewData["Title"] = "Edit";
}
<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">EDIT SEC FAMILY MEMBER</h3>


    </div>
    <!-- /.card-header -->
    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="ParticipantFamiliesOnSEC.Id" />
                    <div class="form-group">
                        <label asp-for="ParticipantFamiliesOnSEC.ParticipantId" class="control-label">ALUMNI</label>
                        <select asp-for="ParticipantFamiliesOnSEC.ParticipantId" class="form-control" asp-items="ViewBag.PId"></select>
                    </div>

                    <div class="form-group">
                <label asp-for="ParticipantFamiliesOnSEC.Date" class="control-label"></label>
                <input asp-for="ParticipantFamiliesOnSEC.Date" class="form-control" />
                <span asp-validation-for="ParticipantFamiliesOnSEC.Date" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ParticipantFamiliesOnSEC.SubCategoryFamiliesOnSECId" class="control-label"></label>
                <select asp-for="ParticipantFamiliesOnSEC.SubCategoryFamiliesOnSECId" class="form-control" asp-items="ViewBag.SubCategoryFamiliesOnSECId"></select>
                <span asp-validation-for="ParticipantFamiliesOnSEC.SubCategoryFamiliesOnSECId" class="text-danger"></span>
            </div>
                    <div class="form-group">
                        <input type="submit" value="Submit" class="btn btn-primary" />
                        <a asp-page="./Index" class="btn bg-gradient-warning btn-sm" style="float:right;">Back</a>

                    </div>
                </form>
            </div>
        </div>


        <!-- /.row -->
    </div>
    <!-- /.card-body -->
    <div class="card-footer">
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
