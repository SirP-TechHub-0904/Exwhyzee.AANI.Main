@page
@model Exwhyzee.AANI.Web.Areas.Main.Pages.ExecutivePage.Members.EditModel

@{
    ViewData["Title"] = "Edit";
}

<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">EDIT PAST EXECUTIVE MEMBER</h3>


    </div>
    <!-- /.card-header -->
    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="PastExecutiveMember.Id" />
            <div class="form-group">
                <label asp-for="PastExecutiveMember.ParticipantId" class="control-label"></label>
                        <select asp-for="PastExecutiveMember.ParticipantId" class="form-control select2 select2-primary js-example-basic-single" data-dropdown-css-class="select2-primary" asp-items="ViewBag.ParticipantId"></select>
                <span asp-validation-for="PastExecutiveMember.ParticipantId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PastExecutiveMember.Date" class="control-label"></label>
                <input asp-for="PastExecutiveMember.Date" class="form-control" />
                <span asp-validation-for="PastExecutiveMember.Date" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PastExecutiveMember.Position" class="control-label"></label>
                <input asp-for="PastExecutiveMember.Position" class="form-control" />
                <span asp-validation-for="PastExecutiveMember.Position" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PastExecutiveMember.PastExecutiveYearId" class="control-label"></label>
                <select asp-for="PastExecutiveMember.PastExecutiveYearId" class="form-control" asp-items="ViewBag.PastExecutiveYearId"></select>
                <span asp-validation-for="PastExecutiveMember.PastExecutiveYearId" class="text-danger"></span>
            </div>
                    <div class="form-group">
                        <input type="submit" value="Submit" class="btn btn-primary" />
                        <a asp-page="./Index" class="btn bg-gradient-warning btn-sm" style="float:right;">Back</a>

                    </div>
                </form>
            </div>
        </div>


        <!-- /.row -->
    </div>
    <!-- /.card-body -->
    <div class="card-footer">
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
