@page
@model Exwhyzee.AANI.Web.Areas.Main.Pages.ISliders.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">DETAILS SLIDER</h3>

    </div>
    <!-- /.card-header -->

    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Slider.Url)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Slider.Url)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Slider.Key)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Slider.Key)
                    </dd>

                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Slider.SortOrder)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Slider.SortOrder)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Slider.Show)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Slider.Show)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Slider.Title)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Slider.Title)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Slider.Text)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Slider.Text)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Slider.MiniTitle)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Slider.MiniTitle)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Slider.SliderLinkUrl)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Slider.SliderLinkUrl)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Slider.SliderLinkUrlTitle)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Slider.SliderLinkUrlTitle)
                    </dd>


                </dl>
            </div>
        </div>
        <!-- /.card-body -->
        <div class="card-footer">
            <a href="javascript:history.back()" class="btn bg-gradient-warning btn-sm">Back</a>

        </div>
    </div>
</div>