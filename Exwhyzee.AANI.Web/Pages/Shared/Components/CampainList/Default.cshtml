@model ICollection<Exwhyzee.AANI.Domain.Models.Campain>


@if (Model.Count > 0)
{
    <div class="row">

        @foreach (var item in Model)
        {


<div class="col-md-4">
                <div class="item">
                    <div class="team-member">
                        <div class="team-member-img" style="margin:0 auto;text-align:center;">
                            @if (!String.IsNullOrEmpty(item.ImageUrl))
                            {
                                <img src="@item.ImageUrl" alt="img" style="margin:0 auto;text-align:center;">

                            }
                        </div>
                        <div class="team-member-text">
                            <h4><a href="#">@item.Participant.Fullname</a></h4>
                            <span>SEC @item.Participant.SEC.Number (@item.Participant.SEC.Year)</span>
                            <p>
                                @item.CampaignQoute
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        }
    </div>

}
else
{
    <span>NO CANDIDATE YET</span>
}
