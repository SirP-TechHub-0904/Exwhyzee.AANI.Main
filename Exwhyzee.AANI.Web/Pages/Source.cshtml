@page "/Source/{id}/{title}"
@using System.Text.RegularExpressions;

@{
    Regex regex = new Regex("[^a-zA-Z0-9]");
}
@model Exwhyzee.AANI.Web.Pages.SourceModel
@{
    ViewData["Title"] = Model.WebPage.Title;

    ViewData["PageName"] = Model.WebPage.Title;


}


<section class="page-header">
    <div class="page-header__bg"
         style='background-image: url("@(string.IsNullOrEmpty(Model.ContactSettings?.BreadcrumImageUrl) ? "default-image.jpg" : Model.ContactSettings.BreadcrumImageUrl)")'>
    </div>
    
    <div class="container">
        <div class="page-header__inner">
            <h2>@Model.WebPage.Title</h2>
            <div class="thm-breadcrumb__box">
                <ul class="thm-breadcrumb list-unstyled">
                    <li><a href="/"><i class="fas fa-home"></i>Home</a></li>
                    <li><span class="icon-right-arrow-1"></span></li>
                    <li class="active">@Model.WebPage.Title</li>
                </ul>
            </div>
        </div>
    </div>
</section>
 

@await Html.PartialAsync("_PageSections", new { position = "", pageset = "", id = Model.WebPage.Id, secid = 0 })

